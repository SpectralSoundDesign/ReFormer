<%= form_with model: @user do |form| %>
<%= form.label "Username:" %>
<%= form.text_field :username, value: "someusername22" %>
<%= form.label "Email:" %>
<%= form.text_field :email, value: "example@email.com" %>
<%= form.label "Password:" %>
<%= form.password_field :password, value: "12345" %>
<%= form.submit "Update" %>
<% end %>